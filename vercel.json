{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ],
    "functions": {
        "dist/server.js": {
            "maxDuration": 60
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}